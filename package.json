    "lint-staged": {
        "*.tsx": [
            "eslint --cache --fix",
            "prettier --write --cache --ignore-unknown"
        ]
    },
    "scripts": {
        "nx": "nx",
        "build": "nx run-many --target=build --all",
        "dev": "NEXT_IGNORE_INCORRECT_LOCKFILE=true nx run-many --target=dev --all --turbopack",
        "lint": "nx run-many --target=lint --all --no-error-on-unmatched-pattern",
        "test": "nx run-many --target=test --all",
        "e2e": "nx run-many --target=e2e --all",
        "storybook": "nx storybook storybook-app",
        "affected:build": "nx affected:build",
        "affected:dev": "nx affected --target=dev",
        "affected:lint": "nx affected:lint --base=origin/main --exclude=libs/dictionaries --no-error-on-unmatched-pattern",
        "affected:test": "nx affected:test --base=origin/main --exclude=libs/dictionaries",
        "affected:e2e": "nx affected:e2e --base=origin/main",
        "affected:dep-graph": "nx graph --affected --base=origin/main",
        "affected:apps": "nx show projects --affected --base=origin/main --type=app",
        "affected:libs": "nx show projects --affected --base=origin/main --type=lib",
        "affected:build:ci": "nx affected:build --base=origin/main --head=HEAD",
        "affected:dev:ci": "nx affected --target=dev --base=origin/main --head=HEAD",
        "affected:lint:ci": "nx affected:lint --base=origin/main --head=HEAD",
        "affected:test:ci": "nx affected:test --base=origin/main --head=HEAD",
        "affected:e2e:ci": "nx affected:e2e --base=origin/main --head=HEAD",
        "affected:dep-graph:ci": "nx graph --affected --base=origin/main --head=HEAD",
        "affected:apps:ci": "nx show projects --affected --base=origin/main --head=HEAD --type=app",
        "affected:libs:ci": "nx show projects --affected --base=origin/main --head=HEAD --type=lib",
        "format": "nx format:write"
    },
