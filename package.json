{
  "name": "fdk-frontend",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=20.11.1"
  },
  "packageManager": "yarn@4.2.2",
  "lint-staged": {
    "*.tsx": [
      "eslint --cache --fix",
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "scripts": {
    "build": "nx run-many --target=build --all",
    "dev": "nx run-many --target=dev --all",
    "lint": "nx run-many --target=lint --all --no-error-on-unmatched-pattern",
    "test": "nx run-many --target=test --all",
    "e2e": "nx run-many --target=e2e --all",
    "affected:build": "nx affected:build",
    "affected:dev": "nx affected:dev",
    "affected:lint": "nx affected:lint --base=develop --exclude=libs/dictionaries --no-error-on-unmatched-pattern",
    "affected:test": "nx affected:test --base=develop --exclude=libs/dictionaries",
    "affected:e2e": "nx affected:e2e --base=develop",
    "affected:dep-graph": "nx affected:dep-graph --base=develop",
    "affected:apps": "nx affected:apps --base=develop",
    "affected:libs": "nx affected:libs --base=develop",
    "affected:build:ci": "nx affected:build --base=origin/main --head=HEAD",
    "affected:dev:ci": "nx affected:dev --base=origin/main --head=HEAD",
    "affected:lint:ci": "nx affected:lint --base=origin/main --head=HEAD",
    "affected:test:ci": "nx affected:test --base=origin/main --head=HEAD",
    "affected:e2e:ci": "nx affected:e2e --base=origin/main --head=HEAD",
    "affected:dep-graph:ci": "nx affected:dep-graph --base=origin/main --head=HEAD",
    "affected:apps:ci": "nx affected:apps --base=origin/main --head=HEAD",
    "affected:libs:ci": "nx affected:libs --base=origin/main --head=HEAD",
    "matrix": "nx print-affected --type=app --select=projects --plain|tr -d '\\n'|jq -cRs 'split(\", \")'",
    "format": "nx format:write",
    "postinstall": "husky install",
    "storybook": "nx storybook storybook-host"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.9.0",
    "@digdir/designsystemet-css": "^0.10.0",
    "@digdir/designsystemet-react": "^0.63.0",
    "@digdir/designsystemet-theme": "^0.15.3",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@navikt/aksel-icons": "^6.5.0",
    "classnames": "^2.5.1",
    "framer-motion": "^11.2.11",
    "leaflet": "^1.9.4",
    "next": "14.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.1",
    "tslib": "^2.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.0.0",
    "@nx/devkit": "18.2.4",
    "@nx/eslint": "18.2.4",
    "@nx/eslint-plugin": "18.2.4",
    "@nx/jest": "18.2.4",
    "@nx/js": "18.2.4",
    "@nx/next": "18.2.4",
    "@nx/playwright": "18.2.4",
    "@nx/react": "18.2.4",
    "@nx/storybook": "18.2.4",
    "@nx/web": "18.2.4",
    "@nx/workspace": "18.2.4",
    "@playwright/test": "^1.44.0",
    "@storybook/addon-essentials": "7.6.19",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/core-server": "7.6.19",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "7.6.19",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@swc-node/register": "~1.9.0",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.4.13",
    "@swc/helpers": "~0.5.8",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.1",
    "@types/jest": "^29.5.12",
    "@types/jest-axe": "^3.5.9",
    "@types/leaflet": "^1.9.12",
    "@types/lodash": "^4.17.0",
    "@types/negotiator": "^0.6.3",
    "@types/node": "20.12.7",
    "@types/react": "18.2.76",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": ">=10",
    "nx": "18.2.4",
    "playwright-core": "^1.44.1",
    "postcss": "^8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.9",
    "prettier": "3.2.5",
    "prettier-plugin-sort-json": "^4.0.0",
    "react-markdown": "^9.0.1",
    "sass": "^1.77.6",
    "storybook": "7.6.19",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  }
}
